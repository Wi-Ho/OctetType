{
    "name": "wi-ho/size-type",
    "description": "Symfony Form Type to select size with multiple units.",
    "type": "library",
    "license": "MIT",
    "keywords": ["Symfony", "Form", "Size"],
    "homepage": "https://github.com/Wi-Ho/SizeType",
    "authors": [
        {
            "name": "Florent Morselli",
            "homepage": "https://github.com/Spomky"
        },{
            "name": "All contributors",
            "homepage": "https://github.com/Wi-Ho/SizeType/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "SizeType\\Form\\":  "src/"
        }
    },
    "require": {
        "php": ">=5.6",
        "symfony/form": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "satooshi/php-coveralls": "^1.0",
        "humbug/humbug": "1.0.*@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
